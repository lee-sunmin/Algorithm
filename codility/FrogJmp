class Solution {
    public int solution(int X, int Y, int D) {
        // write your code in Java SE 8
        
        
        int tmp = Y-X;
        int res = 0;
        
        if(tmp%D != 0){
            res = tmp/D +1;
        }else{
            res = tmp/D;
        }
        
        return res;
    }
}
